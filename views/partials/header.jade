#header
  .title
    a(href="/") QandA

  .loginout
    - if (this.getAuthDetails() && this.getAuthDetails().user)
      = this.getAuthDetails().user.name
      &nbsp;|&nbsp;
      a(href="/logout") logout
    - else
      a(href=this.app.settings.createAuthenticateUrl(this.url)) login
      &nbsp;|&nbsp;
      a(href="/register") register
